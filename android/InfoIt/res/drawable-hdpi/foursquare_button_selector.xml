<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="true">
    <shape>
        <gradient
            android:angle="90"
            android:startColor="#1C95FF"
            android:endColor="#8FCBFF"
            android:type="linear" />
    </shape>
</item>
<item android:state_pressed="false">
    <shape>
        <gradient
            android:angle="90"
            android:startColor="#006DCC"
            android:endColor="#3283C9"
            android:type="linear" />
    </shape>
</item>
</selector>